{
  "jsc": {
      "parser": {
          "syntax": "typescript",
          "decorators": true
      },
      "target": "es2017",
      "baseUrl": "src",
      "keepClassNames": true,
      "transform": {
        "legacyDecorator": true,
        "decoratorMetadata": true
      },
      "paths": {
          "#core/*": [
              "./core/*"
          ]
      }
      
  }
}